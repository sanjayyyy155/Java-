
import java.util.*;

public class maxof3num{
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

    
        int a = scanner.nextInt();
        int b = scanner.nextInt();
        int c = scanner.nextInt();

        int max, min;
        if (a >= b && a >= c) {
            max = a;
        } else if (b >= a && b >= c) {
            max = b;
        } else {
            max = c;
        }
        
        System.out.println("Max=" + max);
    }
}
